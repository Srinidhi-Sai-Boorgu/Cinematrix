  <body class="bg-black">
    <main>
      <div class="container">
        <section
          class="section register min-vh-100 d-flex flex-column align-items-center justify-content-center py-0"
        >
          <div class="container">
            <div class="row justify-content-center">
              <div
                class="col-lg-4 col-md-6 d-flex flex-column align-items-center justify-content-center"
              >
                <div class="d-flex justify-content-center py-3">
                  <a
                    href="/signup"
                    class="logo d-flex align-items-center w-auto"
                  >
                    <img src="logo/logo.png" alt="" />
                    <span class="text-white d-lg-block">CINEMATRIX</span>
                  </a>
                </div>

                <div class="card mb-3" style="width: 600px;">
                  <div class="card-body">
                    <div class="pt-2 pb-1">
                      <h5 class="card-title text-center pb-0 fs-4">
                        Create an Account/  <a href="/signin">Log in</a>
                        </p>
                        <% if (message) { %>
                          <p class="text-center small" style="margin: 0; color:red; font-size: 1.2rem; font-weight:600">
                             <%= message %> 
                          </p>
                          <% }%>
                      </h5>
                    </div>

                    <form
                      class="row g-3 needs-validation"
                      novalidate
                      action="/register_data_save"
                      method="post"
                    >
                      <div class="col-12">
                        <label for="yourUsername" class="form-label text-black"
                          >Username</label
                        >
                        <input
                          type="text"
                          name="name"
                          class="form-control"
                          id="yourUsername"
                          required
                        />
                        <div class="invalid-feedback">
                          Please, enter your name!
                        </div>
                      </div>

                      
                      <div class="col-12">
                        <label for="youremail" class="form-label text-black"
                        >E-Mail</label
                        >
                        <div class="input-group has-validation">
                          <span class="input-group-text" id="inputGroupPrepend"
                            >@</span
                            >
                          <input
                            type="text"
                            name="email"
                            class="form-control"
                            id="youremail"
                            required
                            />
                          <div class="invalid-feedback">
                            Please choose a email.
                          </div>
                        </div>
                      </div>
                      
                      <div class="col-6">
                        <label for="yourdob" class="form-label text-black"
                          >Date of Birth</label
                        >
                        <input
                          type="Date"
                          name="dob"
                          class="form-control"
                          id="yourdob"
                          style="width: 100%;"
                          required
                        />
                        <div class="invalid-feedback">
                          Please enter Date Of Birth!
                        </div>
                      </div>

                      <div class="col-6">
                        <label for="yourcountry" class="form-label text-black"
                          >Country</label
